import java.util.*;

public class People {
    private String firstName;
    private String lastName;
    private int age;
    private String gender;

    // Constructor
    public People(String _firstName, String _lastName, int _age, String _gender) {
        this.firstName = _firstName;
        this.lastName = _lastName;
        this.age = _age;
        this.gender = _gender;
    }

    // Accessors
    public String getFirstName() {
        return this.firstName;
    }

    public String getLastName() {
        return this.lastName;
    }

    public String getName() {
        return this.firstName + " " + this.lastName;
    }

    public int getAge() {
        return this.age;
    }

    public String getGender() {
        return this.gender;
    }

    // Mutators
    public void setFirstName(String _firstName) {
        this.firstName = _firstName;
    }

    public void setSecondName(String _lastName) {
        this.lastName = _lastName;
    }

    public void setAge(int _age) {
        this.age = _age;
    }

    public void setGender(String _gender) {
        this.gender = _gender;
    }

    // Check if a person is already in a specific map
    // before calling addFriend() or addChild()
    // or addParents() method.
    
}
